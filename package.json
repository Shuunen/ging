  "scripts": {
    "build": "pnpm build:vue && pnpm mark:html && pnpm mark:js && echo build success",
    "build:vue": "vue-cli-service build",
    "check": "pnpm lint && pnpm build && pnpm test && echo check success",
    "dev": "vue-cli-service serve",
    "lint": "repo-check && pnpm lint:ts && pnpm lint:vue && echo lint success",
    "lint:fake": "eslint --cache .ts && echo remove me",
    "lint:vue": "vue-tsc --noEmit && vue-cli-service lint --fix --cache && echo lint:vue success",
    "lint:ts": "tsc --noEmit && echo lint:ts success",
    "mark:html": "unique-mark dist/index.html",
    "mark:js": "unique-mark dist/js/app.*.js",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm check",
    "serve": "vue-cli-service serve",
    "test": "vitest --run --coverage && echo test success",
    "test:update": "vitest --run --update && echo test update success",
    "test:watch": "vitest --coverage"
  },
  "version": "0.5.0"
