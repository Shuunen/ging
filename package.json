  "scripts": {
    "build": "pnpm build:vue && pnpm mark:html && pnpm mark:js && echo build success",
    "build:vue": "vue-cli-service build",
    "check": "pnpm lint && pnpm build && pnpm test && echo check success",
    "dev": "vue-cli-service serve",
    "lint": "tsc --noEmit && repo-check && pnpm lint:vue && echo lint success",
    "lint:fake": "eslint --cache .ts && echo remove me",
    "lint:vue": "vue-cli-service lint --fix --cache",
    "mark:html": "unique-mark dist/index.html",
    "mark:js": "unique-mark dist/js/app.*.js",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm check",
    "serve": "vue-cli-service serve",
    "test": "c8 uvu -r module-alias/register -r tsm tests && echo test success",
    "test:watch": "watchlist src tests -eager -- pnpm test"
  },
  "version": "0.5.0"
